{
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.rulers": [120],
    "files.insertFinalNewline": true,
    "files.encoding": "utf8",
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "lib/*": true,
        "coverage/*": true
    },
    "files.trimTrailingWhitespace": true,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "typescript.preferences.quoteStyle": "double"
}
